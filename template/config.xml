<?xml version="1.0" encoding="UTF-8"?>
<config>
    <ips number="3"><!-- 用户设定的待抓取网站ip地址，number值表示本次并发抓取的网站数，最小/默认为1，最大为5 -->
        <ip>https://www.xicidaili.com/nn/1<!-- ip地址中需要包含页码占位符，后端需要重新构造url，方便抓取更多代理ip --></ip>
        <ip>https://www.xicidaili.com/nn/2</ip>
        <ip>https://www.xicidaili.com/nn/3</ip>
    </ips>
    <dom_1><!-- 根据ips中number的值生成相应数量的dom结构，dom_1代表第一个ip地址对应的网页结构，依次类推 -->
        <label_1><!-- 标签1，通常代表table标签 -->
            <name>table1</name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_1>
        <label_2><!-- 标签2，通常代表thead标签，表示表格头 -->
            <name>thead1<!-- 可以指定为null，表示无表格头 --></name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_2>
        <label_3><!-- 标签3，通常代表tbody标签，表示表格数据，当thead为null时，表格头通常包含在tbody中 -->
            <name>tbody1</name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_3>
    </dom_1>
    <dom_2>
        <label_1><!-- 标签1，通常代表table标签 -->
            <name>table2</name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_1>
        <label_2><!-- 标签2，通常代表thead标签，表示表格头 -->
            <name>thead2<!-- 可以指定为null，表示无表格头 --></name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_2>
        <label_3><!-- 标签3，通常代表tbody标签，表示表格数据，当thead为null时，表格头通常包含在tbody中 -->
            <name>tbody2</name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_3>
    </dom_2>
    <dom_3>
        <label_1><!-- 标签1，通常代表table标签 -->
            <name>table3</name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_1>
        <label_2><!-- 标签2，通常代表thead标签，表示表格头 -->
            <name>thead3<!-- 可以指定为null，表示无表格头 --></name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_2>
        <label_3><!-- 标签3，通常代表tbody标签，表示表格数据，当thead为null时，表格头通常包含在tbody中 -->
            <name>tbody3</name>
            <attribute>
                <name>null<!-- 可以指定为null，表示无属性 --></name>
                <value>{属性值}</value>
            </attribute>
        </label_3>
    </dom_3>
</config>